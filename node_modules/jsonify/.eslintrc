{
    "root": true,

    "extends": "@ljharb",

    "rules": {
        "complexity": 0,
        "consistent-return": 1,
        "func-style": [2, "declaration"],
        "max-depth": 1,
        "max-lines-per-function": 0,
        "max-statements": 0,
        "multiline-comment-style": 0,
        "no-control-regex": 1,
        "no-misleading-character-class": 1,
        "no-restricted-syntax": 1,
        "no-throw-literal": 0,
        "no-unmodified-loop-condition": 0,
        "sort-keys": 0,
    },
}
